{
  "name": "consent-revoke",
  "version": "1.0.0",
  "description": "撤回同意和账号注销云函数",
  "main": "index.js",
  "dependencies": {},
  "cloudfunction-config": {
    "memorySize": 256,
    "timeout": 30,
    "triggers": [
      {
        "name": "process-deletions",
        "type": "timer",
        "config": "0 0 2 * * * *"
      }
    ],
    "path": "/consent/revoke",
    "keepRunningAfterReturn": true
  }
}
